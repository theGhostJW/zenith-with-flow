  "scripts": {
    "run": "node ./dist/index.js",
    "build": "(if exist dist rmdir dist /s/q) && mkdir dist && babel src test --extensions \".es6,.js,.es,.jsx,.mjs,.ts\" --out-dir dist --copy-files --source-maps --verbose",
    "debug": "yarn run build && node inspect ./dist/test/sum.test.js",
    "launchDetachedWdioServerInstance": "tsc && node ./scripts/launchWebDriverIO.js",
    "test": "mocha --require ts-node/register ./src/test/**.test.ts",
    "testWatch": "node ./scripts/yarnWatch.js test",
    "testAll": "mocha --require ts-node/register ./src/test/**.test.ts ./src/test/**/*.integration.ts",
    "endpoints": "mocha --require ts-node/register ./src/test/**/*.endpoints.ts ./testCases/**/*.endpoints.ts",
    "testAllWatch": "node ./scripts/yarnWatch.js testAll",
    "testCoverage": "nyc --reporter=html --reporter=text mocha --compilers js:@babel/register ./src/test/**/*.test.js ./src/test/**/*.integration.js",
    "fixBuild": "yarn add @babel/cli @babel/preset-flow @babel/preset-env gaze flow-bin nyc istanbul-reports --dev",
    "updateExternalFlowDefs": "yarn install && yarn flow-typed install --overwrite"
  },
  "nyc": {
    "exclude": [
      "**/*.integration.js",
      "**/*.test.js",
      "**/*.endpoints.js"
    ]
  }
}
