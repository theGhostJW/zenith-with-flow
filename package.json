{
  "name": "ZWTF",
  "version": "1.0.0",
  "description": "Zenith with Types from Flow",
  "main": "index.js",
  "repository": "https://theghostjw@bitbucket.org/theghostjw/zwft.git",
  "author": "theGhostJW <theghostjw@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "color": "^2.0.0",
    "debug": "^2.6.8",
    "del": "^3.0.0",
    "file-system": "^2.2.2",
    "flow-typed": "^2.1.5",
    "js-yaml": "^3.9.1",
    "lodash": "^4.17.4",
    "message": "^0.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "string": "^3.3.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-node": "^5.1.1",
    "flow-bin": "^0.53.1",
    "flow-types": "^1.0.0",
    "gaze": "^1.1.2",
    "istanbul": "^0.4.5",
    "istanbul-reports": "^1.1.1",
    "nyc": "^11.1.0"
  },
  "scripts": {
    "run": "node ./dist/index.js",
    "tryScript": "node ./scripts/runWatchUnitTests.js",
    "build": "babel ./src --source-maps-inline -d ./dist",
    "prepublish": "yarn run build",
    "debug": "yarn run prepublish && node inspect ./dist/test/sum.test.js",
    "test": "mocha --compilers js:babel-core/register ./src/test/**/*.test.js",
    "testWatch": "mocha --compilers js:babel-core/register -w ./src/test/**/*.test.js",
    "testAll": "mocha --compilers js:babel-core/register ./src/test/**/*.test.js ./src/test/**/*.integration.js",
    "testAllWatch": "mocha --compilers js:babel-core/register -w ./src/test/**/*.test.js ./src/test/**/*.integration.js",
    "testCoverage": "nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./src/test/**/*.test.js ./src/test/**/*.integration.js",
    "fixBabel": "yarn add babel-cli babel-preset-flow babel-preset-env babel-preset-node gaze flow-bin nyc istanbul-reports --dev"
  },
  "nyc": {
    "exclude": [
      "**/*.integration.js",
      "**/*.test.js"
    ]
  }
}
