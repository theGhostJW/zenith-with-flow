{
  "name": "ZWTF",
  "version": "1.0.0",
  "description": "Zenith with Types from Flow",
  "main": "index.js",
  "repository": "https://theghostjw@bitbucket.org/theghostjw/zwft.git",
  "author": "theGhostJW <theghostjw@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.0",
    "csv-parse": "^1.2.1",
    "deasync": "^0.1.12",
    "debug": "^2.6.8",
    "del": "^3.0.0",
    "file-system": "^2.2.2",
    "flow-typed": "^2.1.5",
    "js-yaml": "^3.9.1",
    "lodash": "^4.17.4",
    "lodash-deep": "^2.0.0",
    "message": "^0.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "mssql": "^4.1.0",
    "n-readlines": "^0.2.8",
    "node-zip": "^1.1.1",
    "psnode": "^0.0.1",
    "sqlite3": "^3.1.13",
    "string": "^3.3.3",
    "sync-request": "^4.1.0",
    "tedious": "^2.1.5",
    "winston": "^2.3.1",
    "xml-formatter": "^1.0.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-node": "^5.1.1",
    "flow-bin": "^0.63.1",
    "flow-types": "^1.0.0",
    "gaze": "^1.1.2",
    "istanbul": "^0.4.5",
    "istanbul-reports": "^1.1.3",
    "nyc": "^11.4.1"
  },
  "scripts": {
    "run": "node ./dist/index.js",
    "build": "babel ./src --source-maps-inline -d ./dist && yarn flow-copy-source src dist",
    "prepublish": "yarn run build",
    "debug": "yarn run prepublish && node inspect ./dist/test/sum.test.js",
    "test": "mocha --compilers js:babel-core/register ./src/test/**/*.test.js",
    "testWatch": "node ./scripts/yarnWatch.js test",
    "testAll": "mocha --compilers js:babel-core/register ./src/test/**/*.test.js ./src/test/**/*.integration.js",
    "endPoints": "mocha --compilers js:babel-core/register ./src/test/**/*.endpoints.js ./testCases/**/*.endpoints.js",
    "testAllWatch": "node ./scripts/yarnWatch.js testAll",
    "testCoverage": "nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./src/test/**/*.test.js ./src/test/**/*.integration.js",
    "fixBuild": "yarn add babel-cli babel-preset-flow babel-preset-env babel-preset-node gaze flow-bin nyc istanbul-reports --dev"
  },
  "nyc": {
    "exclude": [
      "**/*.integration.js",
      "**/*.test.js"
    ]
  }
}
